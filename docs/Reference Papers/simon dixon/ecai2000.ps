0.5 0.5 scale
/branch { 2 copy } def
/start  { newpath 2 copy 5 0 360 arc closepath fill } def
/cont   {
	newpath
	5 1 roll
	4 2 roll moveto
	2 copy exch 5 sub exch lineto
	closepath stroke
	newpath
	2 copy 5 0 360 arc
	closepath
	3 2 roll { fill } { stroke } ifelse
	exch 5 add exch
	} def
/stop   {
	newpath
	moveto
	3 { 10 0 rmoveto 10 0 rlineto } repeat
	closepath stroke
	} def
/plainstop { pop pop } def

/beatLocation2 {
	240 % vertical location
	2 setlinewidth
	newpath
	% 2 copy moveto 0 -5 rmoveto 0 10 rlineto
	2 copy 5 0 360 arc
	closepath stroke
	2 copy 10 add exch 6 sub exch moveto
	3 2 roll show
} def
/beatLocation {
	240 % vertical location
	2 setlinewidth
	newpath
	2 copy 5 0 360 arc
	closepath fill
	2 copy 10 add exch 6 sub exch moveto
	3 2 roll show
} def
/dashedline { %  n x y
	5 sub moveto
	{ 0 10 rlineto 0 10 rmoveto } repeat
} def
/window { %  n x1 x2 y
	1 setlinewidth
	newpath
	dup 3 1 roll
	2 copy moveto
	4 2 roll
	2 copy lineto
	4 index 3 1 roll
	dashedline
	dashedline
	closepath stroke
} def

/Helvetica-Italic findfont 16 scalefont setfont
470 250 moveto (Time) show

/Helvetica-Normal findfont 16 scalefont setfont

1 setlinewidth

30 236 moveto (Events) show
2 setlinewidth
newpath			% Events line
	90 240 moveto
	405 0 rlineto
	-5 5 rlineto
	0 -10 rmoveto
	5 5 rlineto
closepath stroke

(A) 130 beatLocation
(B) 230 beatLocation
(C) 330 beatLocation2
(D) 430 beatLocation2

2 setlinewidth

160 194 moveto (Inner windows:) show
3 320 340 200 window
3 420 440 200 window
160 154 moveto (Outer windows:) show
5 310 370 160 window
5 410 470 160 window

